(define (crash!)
  (/ 0 0))

(define (test-lambda)
  (let* ((f (lambda (n) n))
          (fr (f 6)))
    (va-all? id
      (= 2 ((lambda (n) (+ n n)) 1))
      (< 2 ((lambda (n) (+ n n)) 2))
      (= 2 ((lambda (n) n) 2))
      ((lambda (n) #t) 2)
      (= (f 2) 2) 
      (= (f 2) 2) 
      (= (f 2) 2)
      (= fr 6)
      )))

