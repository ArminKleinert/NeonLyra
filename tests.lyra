      (always-true (box-set! b 16))
      (box? b)
      (= b.? 16)
      (= (unbox b) 16)
      (int? b.?))))

(define (test-read-string)
  (va-all? id
    (nothing? (read-string "Nothing"))
    (list? (read-string "(a c b)"))
    (all? symbol? (read-string "(a c b)"))
    (vector? (read-string "[1 2 3 4 5 6]"))
    (boolean? (read-string "#t"))
    (boolean? (read-string "#f"))
    (int? (read-string "1"))
    (float? (read-string "0.7"))))

(define (test-eval)
  (va-all? id
    (eval! #t)
    (not (eval! #f))
    (eval! (quote #t))
    (eval! (quote (= 2 2)))
    ))

(define (test-measure)
  (float? (measure! 10 (lambda () #f))))

(define (test-=)
  (= 1 1)
  (not (= 1 100))
  (= #t #t)
  (not (= #t 100))
  (= #f #f)
  (not (= #f 100))
  (= '() '())
  (not (= '() 100))
  (= [] [])
  (not (= [] 100))
  (= "a" "a")
  (not (= "a" 100))
  (= 's 's)
  (not (= 's 100))
  (= = =)
  (not (= = 100)))

(define (test-/=)
  (/= 1 100)
  (not (/= 1 1))
  (/= #t 100)
  (not (/= #t #t))
  (/= #f 100)
  (not (/= #f #f))
  (/= '() 100)
  (not (/= '() '()))
  (/= [] 100)
  (not (/= [] []))
  (/= "a" 100)
  (not (/= "a" "a"))
  (/= 's 100)
  (not (/= 's 's))
  (/= = 100)
  (not (/= = =)))

