(define (test-box)
  (let1 (b (box 5))
    (va-all? id
      (box? b)
      (= b.? 5)
      (= (unbox b) 5)
      (int? b.?)
      (always-true (box-set! b 16))
      (box? b)
      (= b.? 16)
      (= (unbox b) 16)
      (int? b.?))))

(define (test-unbox)
  (let1 (b (box 5))
    (va-all? id
      (box? b)
      (= b.? 5)
      (= (unbox b) 5)
      (int? b.?)
      (always-true (box-set! b 16))
      (box? b)
      (= b.? 16)
      (= (unbox b) 16)
      (int? b.?))))

(define (test-set-box)
  (let1 (b (box 5))
    (va-all? id
      (box? b)
      (= b.? 5)
